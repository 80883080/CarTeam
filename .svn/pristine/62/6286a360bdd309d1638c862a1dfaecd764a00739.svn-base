		<style type="text/css">
			#manage_car #column div{float: left;margin: 10px 0 10px 10px;}
			#manage_car #column #btn_sub{float: right;margin-right: 50px;}
			#manage_car #content ul{float: left;width: 1100px;margin-left: 20px;}
			#manage_car #content ul li{float: left;width: 1098px; margin-top: 15px;border: 1px #ccd0d9 solid;height: 90px;border-radius: 5px;}
			#manage_car #content ul li .car_num{width: 230px;float: left;height: 60px;margin-top: 15px;border-right: 1px solid #CCD0D9;}
			#manage_car #content ul li .car_info{width: 750px;float: left;height: 60px;margin-top: 15px;border-right: 1px solid #CCD0D9;}
			#manage_car #content ul li .car_btn{width: 110px;float: left;height: 60px;margin-top: 15px;}
			#manage_car #content ul li .car_num h1{font-size: 14px;color: #24a7f5;float: left;height: 14px;margin-top: 8px;padding-left: 25px;}
			#manage_car #content ul li .car_num h1:first-child{width: 100%;padding-left: 15px;}
			#manage_car #content ul li .car_num h1:first-child:before{content: "|";font-weight: 600;padding-right: 5px;}
			#manage_car #content ul li .car_info ul{width: 600px;}
			#manage_car #content ul li .car_info ul li{width: 150px;margin: 10px 0 0 0;border: none;height: 14px;}
			#manage_car #content ul li .car_info ul li p{font-size: 12px;float: left;color: #606a6c;}
			#manage_car #content ul li .car_info ul li span{font-size:12px;float: left;color: #606a6c;}
			#manage_car #content ul li .car_btn div{float: left;margin-left: 8px;margin-top: 10px;}
		</style>
