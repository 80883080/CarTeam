html,body{height: 100%;width: 100%;overflow-x: auto;overflow-y: hidden;}
#monitor_car,#manage_job,#manage_addjob,#manage_car{width: 100%;height: 100%;background: #f2f7fa;min-width: 1180px;}
#monitor_car #column{width: 100%;height: 60px;background: #fff;border-bottom: 1px #ccd1d9 solid;}
#monitor_car #shop{width: 100%;height: 100px;background: #fff;border-bottom: 1px #ccd1d9 solid;}
#monitor_car #content{width: 100%;height: 100%;background: #f2f7fa;overflow: auto;}
#monitor_car #column ul li{float: left;margin-top: 15px;margin-left: 5px;}
#monitor_car #column ul #column_btn p{border-right:0px;cursor: pointer;border-radius: 5px;}
#monitor_car #shop ul{float: left;height: 50px;margin-top: 22px;}
#monitor_car #shop ul li{float: left;border-right: 1px #ccd1d9 solid;height: 50px;}
#monitor_car #shop ul li .status{height: 50px;width: 50px;border-radius:5px;background: #000;margin: 0px 40px;float: left;}
#monitor_car #shop ul li .status h1{width: 100%;text-align: center;font-size: 22px;line-height: 22px;height: 22px;padding-top:5px ;}
#monitor_car #shop ul li .status h2{width: 100%;text-align: center;font-size: 14px;}
#monitor_car #shop ul li .status_blue{background: #23a8f5;}
#monitor_car #shop ul li .status_yellow{background: #fbb03b;}
#monitor_car #shop ul li .status_green{background: #8cc63e;}
#monitor_car #shop ul li .status_gery{background: #5f6a6c;}
#monitor_car #shop ul li ul{height: 51px;float: left;width: 90px;margin: 0;margin-right: 40px;}
#monitor_car #shop ul li ul li{width: 90px;float: left;border: 0px;height: 17px;}
#monitor_car #shop ul li ul li p{float: left;color: #606a6c;}
#monitor_car #shop ul li ul li span{float: right;color: #606a6c;}
#monitor_car #content ul{width: 100%;height: auto;float: left;margin-bottom: 240px;}
#monitor_car #content ul li{width: 100%;float: left;height: 52px;margin-top: 30px;}
#monitor_car #content ul li .tips{width: 257px;height: 50px;float: left;border-radius: 5px;border: 1px #ccd0d9 solid;background: #fff;margin-left: 30px;}
#monitor_car #content ul li .tips h2{float:left;width: 68px;height: 50px;line-height: 50px;border-top-left-radius: 5px;border-bottom-left-radius: 5px;background: #fbb03b;font-size: 14px;text-align: center;}
#monitor_car #content ul li .tips_green h2{background: #8cc63e;}
#monitor_car #content ul li .tips_gery h2{background: #5f6a6c;}
#monitor_car #content ul li .tips h1{float: left;width: 170px;color: #5f6a6c;font-size: 14px;margin: 6px 0 0 10px;}
#monitor_car #content ul li .tips h3{float: left;width: 170px;color: #5f6a6c;font-size: 12px;margin: 0px 0 0 10px;}
#monitor_car #content ul li .line{width: 700px;height: 50px;float: left;margin-left: 20px;position: relative;}
#monitor_car #content ul li .line ul li{float: left;width: 70px;height: 50px;margin: 0;}
#monitor_car #content ul li .more{float: right;width: 50px;height: 50px;border-radius: 5px;background: #23a8f5;margin-right: 30px;cursor: pointer;}
#monitor_car #content ul li .more p{width: 24px;margin: 11px;line-height: 14px;}


#manage_job{min-width: 1270px;}
#manage_job #column{width: 100%;height: 60px;background: #fff;border-bottom: 1px #ccd1d9 solid;}
#manage_job #column ul li{float: left;margin:10px 5px;}
#manage_job #column_btn p{border-right:0px;cursor: pointer;border-radius: 5px;}
#manage_job #content .job{float: left;width: 1110px;height: 160px;background: #f4f8f9;border: 1px solid #ccd0d9;border-radius: 5px;margin: 20px;}
#manage_job #content h1{font-size: 14px;color: #0054b4;width: 100%;text-align: left;float: left;margin-bottom: 10px;margin-left: 20px;margin-top: 5px;font-weight: 600;}
#manage_job #content .job_name{width: 340px;float: left;height: 140px;border-right: 1px solid #ccd0d9;margin-top: 10px;}
#manage_job #content .job_name ul{margin-left: 20px;width: 340px;float: left;}
#manage_job #content .job_name ul li{float: left;width: 170px;}
#manage_job #content .job_name ul li p{float: left;width: 60px;color: #606a6c;}
#manage_job #content .job_name ul li span{float: left;width: 85px;color: #606a6c;}
#manage_job #content .road_name{width: 630px;float: left;height: 140px;margin-top: 10px;}
#manage_job #content .road,.back_road{width: 630px;float: left;margin-top: -20px;margin-bottom: 5px;position: relative;}
#manage_job #content .job_change{width: 120px;float: left;margin-top: 20px;}
#manage_job #content .job_change p{width: 45px;height: 30px;line-height: 30px;text-align: center;float: left;border-radius: 5px;background: #23a8f5;margin-left: 10px;cursor: pointer;}
#manage_job #content .job_change .delect{background: #5f6a6c;}


#manage_addjob #column{width: 100%;background: #5f6a6c;height: 30px;}
#manage_addjob #column p{line-height: 30px;margin-left: 30px;}
#manage_addjob .addjob_div{width: 100%;border-bottom: 1px solid #ccd0d9;}
#manage_addjob #job_info{height: 140px;}
#manage_addjob #job_info p{color: #5f6a6c;} 
#manage_addjob #job_info #info1{float: left;width: 100%;}
#manage_addjob #job_info #info1 div{float: left;margin: 20px 10px;}
#manage_addjob #job_info #info2 div{float: left;margin: -10px 10px;}
#manage_addjob #job_info #info3{float: left;width: 100%;}
#manage_addjob #job_info #info3 div{float:left;margin: 10px;}
#manage_addjob #job_city{height: 215px;}
#manage_addjob #job_car{height: 70px;}
#manage_addjob #addjob1{float: left;margin: 17px 10px;}
#manage_addjob #job_city #job_period{width: 140px;margin: 10px;}
#manage_addjob .job_line{background: #000;}
#manage_addjob #period_line{width: 100%;height: 60px;margin-top: -10px;float: left;}
#manage_addjob #backjob_period{width: 100%;}
#manage_addjob #backjob_period div{float: left;margin: 10px;}
#manage_addjob #changep p{color: #606a6c;font-size: 14px;margin-left: 10px;}


#manage_car #column{width: 100%;height: 60px;background: #fff;border-bottom: 1px #ccd1d9 solid;}
#manage_car #column div{float: left;margin: 10px 0 10px 10px;}
#manage_car #column #btn_sub{float: right;margin-right: 50px;}
#manage_car #content ul{float: left;width: 1100px;margin-left: 20px;}
#manage_car #content ul li{float: left;width: 1098px; margin-top: 15px;border: 1px #ccd0d9 solid;height: 90px;border-radius: 5px;}
#manage_car #content ul li .car_num{width: 230px;float: left;height: 60px;margin-top: 15px;border-right: 1px solid #CCD0D9;}
#manage_car #content ul li .car_info{width: 750px;float: left;height: 60px;margin-top: 15px;border-right: 1px solid #CCD0D9;}
#manage_car #content ul li .car_btn{width: 110px;float: left;height: 60px;margin-top: 15px;}
#manage_car #content ul li .car_num h1{font-size: 14px;color: #24a7f5;float: left;height: 14px;margin-top: 8px;padding-left: 25px;}
#manage_car #content ul li .car_num h1:first-child{width: 100%;padding-left: 15px;}
#manage_car #content ul li .car_num h1:first-child:before{content: "|";font-weight: 600;padding-right: 5px;}
#manage_car #content ul li .car_info ul{width: 600px;}
#manage_car #content ul li .car_info ul li{width: 150px;margin: 10px 0 0 0;border: none;height: 14px;}
#manage_car #content ul li .car_info ul li p{font-size: 12px;float: left;color: #606a6c;}
#manage_car #content ul li .car_info ul li span{font-size:12px;float: left;color: #606a6c;}
#manage_car #content ul li .car_btn div{float: left;margin-left: 8px;margin-top: 10px;}


#setting_car,#manage_user,#count_behavior,#setting_repair,#setting_page{width: 100%;height: 100%;background: #f2f7fa;position: relative;}
#setting_car #column{width: 100%;background: #5f6a6c;height: 30px;}
#setting_car #column p{line-height: 30px;text-align: center;}
#setting_car #content #car_num{height: 60px;width: 100%;background: #fff;border-bottom: 1px solid #d7dbe2;}
#setting_car #content #car_num h1{color: #23a8f5;font-size: 16px;line-height: 60px;margin-left: 30px;}
#setting_car #content #car_info{width: 100%;height: auto;border-bottom: 1px solid #d7dbe2;float: left;}
#setting_car #content #car_info div{float: left;margin: 10px 0 10px 10px;height: auto;}
#setting_car #content #person{width: 80%;float: left;}
#setting_car #content #car_info #person p{font-size: 14px;color: #5d6b6c;margin-left: 5px;}
#setting_car #content #driver_info{float: left;border-bottom:1px solid #D7DBE2 ;height: auto;width: 100%;}
#setting_car #content #driver_info form input{float: left;margin-left: 10px;}
#setting_car #content #driver_info form p{float: left;color: #5f6a6c;margin-top: -2px;margin-left: 5px;}
#setting_car #content #driver_info div{float: left;margin: 10px;}
#setting_car #content #driver_name{width: 90%;}
#setting_car #content #setting_btn{width: 100%;}
#setting_car #content #setting_btn div{float: left; margin: 10px;width: 100px;}
#setting_car #content #setting_btn div p{text-align: center;width: 100px;margin: 0;}

#manage_user #column{width: 100%;height: 60px;background: #fff;border-bottom: 1px #ccd1d9 solid;}
#manage_user #column div{float: left;margin: 10px;}
#manage_user #content{height: 100%;overflow: auto;}
#manage_user #content table{width: 100%;text-align: center;margin-bottom:1px;overflow: hidden;background: #fff;}
#manage_user #content table th{text-align: center;height: 30px;background: #5f6a6c;}
#manage_user #content table td{height: 50px;text-align: center;color: #5c6a6b;border: 1px solid #cdd1da;}
#manage_user #content table td div{float: left;margin-left: 15px;}
#manage_user #content #tr1{min-width: 250px;}
#manage_user #content #tr2{min-width: 150px;}
#manage_user #content #tr3{min-width: 150px;}
#manage_user #content #tr4{min-width: 300px;}
#manage_user #content #tr5{width: 140px;}

#count_behavior #column{width: 100%;height: 60px;background: #fff;border-bottom: 1px #ccd1d9 solid;}
#count_behavior #column div{float: left;margin: 15px;}
#count_behavior #column #search{float: right;margin-right: 100px;}
#count_behavior #content{width: 100%;position: relative;}
#count_behavior #content #behavior ul{width: 80%;height: auto;float: left;border-bottom: 1px solid #cdd1da;}
#count_behavior #content #export{float: left;margin-top: 40px;}
#count_behavior #content #behavior ul li{float: left;width: auto;height: 65px;border-right: 1px #cdd1da solid;margin: 40px 0;}
#count_behavior #content #behavior ul li h1{width: auto;padding:0px 40px 5px 40px;color: #0053b5;font-size: 14px;font-weight: 600;line-height: 14px;}
#count_behavior #content #behavior ul li h2{width: auto;padding:5px 40px 5px 40px;color: #606a6c;font-size: 14px;font-weight: 600;line-height: 14px;}

#setting_repair #column{width: 100%;background: #fff;height: auto;border-bottom: 1px solid #CCD1D9;float: left;}
#setting_repair #column div{float: left;margin: 10px;}
#btn_group{float: left;width: 90%;}
#btn_group div{width: 100px;}
#btn_group p{text-align: center;width: 100px;margin: 0;}


#setting_page #setting_title{width: 90%;height: 30px;margin-left: 5%;border-bottom: 1px #d6dae1 solid;padding-top: 30px;}
#setting_page #setting_title li{float: left;height: 30px;width: 70px;background: #5f6a6c;margin-left: 1px;text-align: center;line-height: 30px;
border-top-left-radius: 5px;border-top-right-radius: 5px;cursor: pointer;}
#setting_page #setting_title .title_select{background: #23a8f5;}

#setting_page #setting_content{width: 90%;float: left;margin-left: 5%;margin-top: 20px;}
#setting_page #setting_content li{width: 100%;display: none;}
#setting_page #setting_content li p{color: #5f696b;font-size: 14px;line-height: 22px;}
#setting_page #setting_content li textarea{width: 300px;height: 150px;border: 1px #d6dae1 solid;margin-top: 20px;color: #5f696b;}
#setting_page #setting_content li .oui_btn{width: 100px;margin-top: 20px;}
#setting_page #setting_content li .oui_btn p{width: 100px;text-align: center;margin: 0;line-height: 30px;}

#setting_page #setting_content .content_select{display: block;}
#setting_page #setting_content .oui_input{width: 265px;margin-top: 20px;}
#setting_page #setting_content .oui_input p {line-height: 30px;width: 80px;text-align: center;}
#setting_page #second{margin-left: 20px;}
#setting_page #password .oui_btn{float: left;}


