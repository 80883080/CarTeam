/*公共样式*/
/*车辆状态轴*/
.line .lines{height: 1px;position: absolute;width: 100%;background: #616a6f;margin: 25px 30px;}
.line ul{float: left;height: 50px;z-index: 100;position: relative;}
.line ul li{float: left;width: 70px;height: 50px;margin: 0;}
.line ul li p{width: 70px;text-align: center;height: 14px;line-height: 14px;color: #5f6a6c;}
.line ul li .point{width: 12px;height: 12px;border-radius: 12px;border:1px #5f6a6c solid;margin: 4px auto;background: #fff;}
.line ul li .point span{width: 10px;height: 10px;background: #23a8f5;float: left;border-radius: 10px;margin: 1px;}
.line ul li .point_blue span{background: #23a8f5;}
.line ul li .point_red span{background: #f15a25;}
.line ul li .point_white span{background: #fff;}
/*输入框样式*/
.oui_btn{width: auto;height: 30px;border: 1px #ccd0d9 solid;border-radius: 4px;background: #f4f8f9;cursor: pointer;overflow: hidden;}
.oui_btn p{height: 100%;line-height: 30px;text-align: center;float: left;margin: 0 10px;color: #5f6a6c;background: #f4f8f9;}
.oui_btn_blue p{color: #fff;background: #23a8f5;}
.oui_btn_white p{background: #fff;}

.oui_input{width: auto;height: 30px;border: 1px #ccd0d9 solid;border-radius: 4px;overflow: hidden;}
.oui_input input{float: left;height: 30px;border: none;border-radius: 4px;text-align: left;padding-left: 10px;color: #5f6a6c;}
.oui_input p{float: left;height: 30px;line-height: 30px;padding: 0 10px;border-right: 1px solid #cad0dc;color: #5f6a6c;background: #f4f8f9;}
.oui_input span{float: left;font-size:12px;line-height: 30px;color: #5f6a6c;}
.oui_input_date input{position: relative;width:auto;height: 30px;border-radius: 5px;}
.oui_input_date input::-webkit-calendar-picker-indicator {position: absolute;left: 0;width: 100px;opacity: 0;height: 100%;width: 100%;}
.oui_input_date input::-webkit-inner-spin-button{ visibility: hidden; }

.oui_select{width: auto;height: 30px;border: 1px #ccd0d9 solid;border-radius: 4px;overflow: hidden;}
.oui_select select{float:left;border: none;background: none;height: 30px;color: #5f6a6c;}
.oui_select p{float: left;height: 100%;line-height: 30px;text-align: center;padding: 0 10px;border-right: 1px #ccd0d9 solid;color: #5f6a6c;background: #f4f8f9;}

.oui_radio{width: auto;height: 30px;}
.oui_radio p{height: 30px;line-height: 30px;float: left;}
.oui_radio label{float: left;margin-left: 10px;height: 30px;line-height: 30px;font-size: 14px;}
.oui_radio input{float: left;margin: 5px;margin-top: 8px;}
